    margin-bottom: 20px;
    padding: 15px;
    background: #f8f9fa;
    border-radius: 8px;
    flex-wrap: wrap;
}

.time-selector {
    display: flex;
    align-items: center;
    gap: 10px;
}

.custom-time-range {
    display: flex;
